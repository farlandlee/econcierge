<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Created At</th>
      <th>Last Modified</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <%= for user <- @users do %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.inserted_at %></td>
      <td><%= user.updated_at %></td>
      <td></td>
    </tr>
  <% end %>
  </tbody>
</table>
